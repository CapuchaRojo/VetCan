{
  "name": "vetcan-api",
  "version": "0.1.0",
  "main": "dist/index.js",
  "scripts": {
    "prepare": "husky",
    "dev": "ts-node-dev --respawn --transpile-only src/index.ts",
    "build": "tsc -p tsconfig.json",
    "start:prod": "node dist/index.js",

    "prisma:generate": "npx prisma@5.22.0 generate",
    "prisma:migrate": "npx prisma@5.22.0 migrate deploy",
    "prisma:validate": "npx prisma@5.22.0 validate",

    "postinstall": "npx prisma@5.22.0 generate",

    "seed": "ts-node src/prisma/seed.ts",

    "test": "dotenv -e .env.test -- npx prisma@5.22.0 generate && dotenv -e .env.test -- jest --runInBand"
}
  },
  "dependencies": {
    "@prisma/client": "5.22.0",
    "bcryptjs": "^2.4.3",
    "bullmq": "^2.0.0",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "express-rate-limit": "^8.2.1",
    "ioredis": "^5.3.2",
    "jsonwebtoken": "^9.0.0",
    "pg": "^8.10.0",
    "twilio": "^4.0.0"
  },
  "devDependencies": {
    "prisma": "5.22.0",
    "@types/cors": "^2.8.19",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.5",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/supertest": "^2.0.16",
    "dotenv": "^17.2.3",
    "dotenv-cli": "^11.0.0",
    "husky": "^9.1.7",
    "jest": "^29.7.0",
    "lint-staged": "^16.2.7",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.0.0"
  },
  "engines": {
    "node": ">=20 <21"
  }
}
