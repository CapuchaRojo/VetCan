{
  "name": "vetcan-api",
  "version": "0.1.0",
  "private": true,
  "main": "dist/index.js",

  "engines": {
    "node": ">=20 <21"
    "npm": ">=9 <12"
  },

  "overrides": {
    "prisma": "5.22.0"
  },

"scripts": {
  "preinstall": "npx only-allow npm && node -e \"const v=require('./package.json').dependencies.prisma; if(!v || !v.startsWith('5.22')){throw new Error('âŒ Prisma version drift detected. This project is pinned to Prisma 5.22.x')}\"",
  "prepare": "husky",

  "dev": "ts-node-dev --respawn --transpile-only src/index.ts",
  "build": "tsc -p tsconfig.json",
  "start:prod": "node dist/index.js",

  "prisma:generate": "npx prisma@5.22.0 generate",
  "prisma:migrate": "npx prisma@5.22.0 migrate deploy",
  "prisma:validate": "npx prisma@5.22.0 validate",

  "postinstall": "npx prisma@5.22.0 generate",
  "test": "npx dotenv-cli -e .env.test -- npx prisma@5.22.0 db push && npx dotenv-cli -e .env.test -- jest --runInBand"
}
  "dependencies": {
    "@prisma/client": "5.22.0",
    "prisma": "5.22.0"
    "bcryptjs": "^2.4.3",
    "bullmq": "^2.0.0",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "express-rate-limit": "^8.2.1",
    "ioredis": "^5.3.2",
    "jsonwebtoken": "^9.0.0",
    "twilio": "^4.0.0"
  },

  "devDependencies": {
    "@types/cors": "^2.8.19",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.5",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/supertest": "^2.0.16",
    "dotenv": "^17.2.3",
    "dotenv-cli": "^11.0.0",
    "husky": "^9.1.7",
    "jest": "^29.7.0",
    "lint-staged": "^16.2.7",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.0.0"
  }
}
